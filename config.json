{
  "workflowApiVersion": "1.1",
  "metaData": {
    "icon": "images/talk.png",
    "category": "custom"
  },
  "type": "REST",
  "userInterfaces": {
    "configModal": {
      "width": 600,
      "height": 400,
      "fullscreen": false
    }
  },
  "configurationArguments": {
    "publish": {
      "url": "https://daesung6756.github.io/mce-ca-webinar/"
    }
  },
  "schema": {
    "arguments": {
      "execute": {
        "inArguments": [
          {
            "ContactKey": "{{Contact.key}}"
          },
          {
            "FirstName": "{{Contact.Attribute.JourneyEntrySource.FirstName}}"
          },
          {
            "LastName": "{{Contact.Attribute.JourneyEntrySource.lastName}}"
          }
        ],
        "url": "https://daesung6756.github.io/mce-ca-webinar/",
        "outArguments": [
          {
            "ContactKey": "{{Contact.key}}"
          },
          {
            "FirstName": "{{Contact.Attribute.JourneyEntrySource.FirstName}}"
          },
          {
            "LastName": "{{Contact.Attribute.JourneyEntrySource.lastName}}"
          }
        ]
      }
    }
  }
}
